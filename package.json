{
    "name": "react-component-generator",
    "displayName": "React Component Generator",
    "publisher": "uditkumar01",
    "icon": "logo/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/uditkumar01/ReactComponentGeneratorExtension.git"
    },
    "author": "Udit Kumar",
    "keywords": [
        "component creator",
        "react component",
        "react component generator",
        "react",
        "javascript",
        "reactjs",
        "vscode",
        "vscode-extension",
        "reactcomponents"
    ],
    "description": "A Cool VSCode extension which lets you create your component's files/folders instantly with boilerplate code. Also automatically handles css file import if needed and exports the component from the index file of Component directory. ",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Snippets",
        "Programming Languages",
        "Education",
        "Formatters",
        "Other"
    ],
    "activationEvents": [
        "onCommand:react-component-generator.helloWorld"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "react-component-generator.helloWorld",
                "title": "React Component Generator"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "14.x",
        "@types/vscode": "^1.56.0",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "fs": "^0.0.1-security",
        "path": "^0.12.7"
    }
}
